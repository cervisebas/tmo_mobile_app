{
  "expo": {
    "name": "TMO",
    "slug": "tmo_app",
    "version": "1.0.0",
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "react-native-edge-to-edge",
        {
          "android": {
            "parentTheme": "Theme.EdgeToEdge.Light",
            "enforceNavigationBarContrast": false
          }
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          },
          "ios": {
            "infoPlist": {
              "UIStatusBarStyle": "UIStatusBarStyleAutomatic",
              "UIViewControllerBasedStatusBarAppearance": true
            }
          }
        }
      ],
      [
        "react-native-bootsplash",
        {
          "assetsDir": "assets/bootsplash",
          "android": {
            "parentTheme": "EdgeToEdge",
            "darkContentBarsStyle": "true"
          }
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.cervisebas.tmo-app"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/foreground-image.png",
        "monochromeImage": "./assets/monochrome-image.png",
        "backgroundColor": "#2957BA"
      },
      "package": "com.cervisebas.tmo_app"
    }
  }
}
